{
  "name": "grab-url",
  "description": "ðŸ“¥ Generate Request to API from Browser",
  "type": "module",
  "version": "1.0.7",
  "author": "vtempest",
  "license": "rights.institute/prosper",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtempest/GRAB-URL"
  },
  "main": "./dist/grab-api.cjs.js",
  "types": "./dist/grab-api.d.ts",
  "module": "./dist/grab-api.es.js",
  "bin": {
    "grab-url": "./src/grab-url.js"
  },
  "exports": {
    ".": {
      "types": "./dist/grab-api.d.ts",
      "import": "./dist/grab-api.es.js",
      "require": "./dist/grab-api.cjs.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.es.js",
      "require": "./dist/icons.cjs.js"
    },
    "./icons/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "src",
    "src/grab-api.ts",
    "src/grab-url.ts",
    "src/icons/cli/spinners.json"
  ],
  "scripts": {
    "down": "bun test command.test.js",
    "make": "npm run make:icons; npm run make:docs; npm run make:main",
    "make:docs": "cd docs && npm run build",
    "make:icons": "npx export-svg-typescript@latest -i ./src/icons/svg -o ./src/icons/svg/index.ts",
    "make:main": "vite build",
    "ship": "npx standard-version --release-as patch; npm publish",
    "check": "vitest --ui",
    "docs": "cd docs && npm run start"
  },
  "devDependencies": {
    "@vitest/ui": "^3.1.4",
    "export-svg-typescript": "^0.1.6",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.4"
  },
  "keywords": [
    "api",
    "axios",
    "request",
    "browser",
    "tanstack",
    "query",
    "post",
    "mock-api",
    "fetch",
    "http"
  ],
  "dependencies": {
    "@types/node": "^24.2.1",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^3.2.0",
    "cli-table3": "^0.6.5",
    "ora": "^8.2.0"
  }
}
